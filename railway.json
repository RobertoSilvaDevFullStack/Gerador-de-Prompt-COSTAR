{
    "build": {
        "builder": "NIXPACKS",
        "buildCommand": "pip install --no-cache-dir -r requirements-deploy.txt"
    },
    "deploy": {
        "startCommand": "uvicorn main_demo:app --host 0.0.0.0 --port $PORT",
        "healthcheckPath": "/status",
        "healthcheckTimeout": 300
    }
}
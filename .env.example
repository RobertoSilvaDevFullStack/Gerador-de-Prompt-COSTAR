# Configurações do Supabase (DESABILITADO - USANDO AUTH LOCAL)
# SUPABASE_URL=https://your-project.supabase.co
# SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Configurações das IAs (Multiple AI Providers)
GEMINI_API_KEY=your_gemini_api_key_here
GROQ_API_KEY=your_groq_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here
COHERE_API_KEY=your_cohere_api_key_here
TOGETHER_API_KEY=your_together_api_key_here

# Configurações do Redis (opcional)
REDIS_URL=redis://localhost:6379

# Configurações de Email (opcional)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password
FROM_EMAIL=your_email@gmail.com

# Configurações de Backup
BACKUP_DIR=./backups

# Configurações JWT
JWT_SECRET_KEY=your_jwt_secret_key_here_generate_a_secure_random_string
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=30
{
    "version": 2,
    "builds": [{
        "src": "api/index.py",
        "use": "@vercel/python"
    }],
    "routes": [{
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "ENVIRONMENT": "production"
    },
    "functions": {
        "api/index.py": {
            "maxDuration": 60,
            "memory": 512
        }
    }
}